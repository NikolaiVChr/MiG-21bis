<PropertyList>

	<key n="81">
            <name>Q</name>
            <desc>Trigger flare release</desc>
            <binding>
                <command>property-assign</command>
                <property>/controls/armament/chaff-trigger</property>
                <value type="bool">true</value>
            </binding>
            <binding>
                <command>property-assign</command>
                <property>/controls/armament/flare-trigger</property>
                <value type="bool">true</value>
            </binding>
            <mod-up>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/armament/chaff-trigger</property>
                    <value type="bool">false</value>
                </binding>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/armament/flare-trigger</property>
                    <value type="bool">false</value>
                </binding>
            </mod-up>
        </key>
	<key n="99">
		<name>c</name>
		<desc>toggle canopy</desc>
		<binding>
			<command>property-toggle</command>
			<property>fdm/jsbsim/systems/canopy/command</property>
		</binding>
	</key>

	<key n="4">
		<name>Ctrl-d</name>
		<desc>Deploy braking parachute</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/chute/deploy</property>
			<value type="int">1</value>
		</binding>
	</key>

	<key n="102">
		<name>f</name>
		<desc>Gun trigger</desc>
		<binding>
			<command>property-assign</command>
			<property>controls/armament/trigger</property>
			<value>true</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>controls/armament/trigger</property>
				<value>false</value>
			</binding>
		</mod-up>
	</key>

	<key n="7">
		<name>Ctrl-g</name>
		<desc>jettison braking parachute</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/chute/deploy</property>
			<value type="int">0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>controls/flight/chute_jettisoned</property>
			<value type="int">1</value>
		</binding>
	</key>

	<key n="109">
		<name>m</name>
		<desc>engage nose wheel steering</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/NWS/engaged</property>
			<value>1</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/systems/NWS/engaged</property>
				<value>0</value>
			</binding>
		</mod-up>
	</key>

	<key n="113">
		<name>q</name>
		<desc>Release selected store</desc>
		<binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/armament/release</property>
			<value>1</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/systems/armament/release</property>
				<value>0</value>
			</binding>
		</mod-up>
	</key>

	<key n="115">
		<name>s</name>
		<desc>Press start button</desc>
		<binding>
			<command>property-toggle</command>
			<property>/controls/engines/engine[0]/start-button</property>
		</binding>
		<mod-up>
			<binding>
				<command>property-toggle</command>
				<property>/controls/engines/engine[0]/start-button</property>
			</binding>
		</mod-up>
	</key>

	<key n="6">
		<name>Ctrl-f</name>
		<desc>Toggle Afterburner Reheat (CLR)</desc>
		<binding>
			<command>property-toggle</command>
			<property>fdm/jsbsim/propulsion/clr</property>
		</binding>
	</key>

	<key n="110">
		<name>n</name>
		<desc>Lock on target</desc>
		<binding>
			<command>nasal</command>
			<script>arm_locking.radar_seekTarget();</script>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/armament/gunsight/damping</property>
			<value>1</value>
		</binding>
		<mod-up>
			<binding>
				<command>property-assign</command>
				<property>/controls/armament/gunsight/damping</property>
				<value>0</value>
			</binding>
		</mod-up>
	</key>

	<key n="78">
		<name>N</name>
		<desc>Unlock Target</desc>
		<binding>
			<command>nasal</command>
			<script>arm_locking.unlockTarget();</script>
		</binding>
	</key>

	<key n="105">
		<name>i</name>
		<desc>rotate throttle drum up grossly</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>controls/armament/gunsight/throttle-drum</property>
			<step>0.05</step>
			<min>0</min>
			<max>1</max>
			<wrap>false</wrap>
		</binding>
	</key>

	<key n="73">
		<name>I</name>
		<desc>rotate throttle drum up fine</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>controls/armament/gunsight/throttle-drum</property>
			<step>0.01</step>
			<min>0</min>
			<max>1</max>
			<wrap>false</wrap>
		</binding>
	</key>

	<key n="107">
		<name>k</name>
		<desc>rotate throttle drum down gross</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>controls/armament/gunsight/throttle-drum</property>
			<step>-0.05</step>
			<min>0</min>
			<max>1</max>
			<wrap>false</wrap>
		</binding>
	</key>

	<key n="75">
		<name>K</name>
		<desc>rotate throttle drum down fine</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>controls/armament/gunsight/throttle-drum</property>
			<step>-0.01</step>
			<min>0</min>
			<max>1</max>
			<wrap>false</wrap>
		</binding>
	</key>

	<key n="83">
		<name>S</name>
		<desc>Activate Smokepod</desc>
		<repeatable>true</repeatable>
		<binding>
			<command>property-toggle</command>
			<property>/controls/smokepod/trigger</property>
		</binding>
	</key>

	<key n="114">
		<name>r</name>
		<desc>Pylon Select Next</desc>
		<repeatable>false</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>/controls/armament/panel/pylon-knob</property>
			<step>1</step>
			<min>0</min>
			<max>11</max>
			<wrap>true</wrap>
		</binding>
	</key>

	<key n="82">
		<name>R</name>
		<desc>Pylon Select Previous</desc>
		<repeatable>false</repeatable>
		<binding>
			<command>property-adjust</command>
			<property>/controls/armament/panel/pylon-knob</property>
			<step>-1</step>
			<min>0</min>
			<max>11</max>
			<wrap>true</wrap>
		</binding>
	</key>

	<key n="1">
		<name>Ctrl+a</name>
		<desc>autoleveller autopilot</desc>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>autopilot.panel_button("level");</script>
		</binding>
	</key>

	<key n="87">
		<name>W</name>
		<desc>autopilot off</desc>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>autopilot.panel_button("off");</script>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/fdm/jsbsim/fcs/elevator-ap-cmd-norm</property>
			<value>0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/fdm/jsbsim/fcs/aileron-ap-cmd-norm</property>
			<value>0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/fdm/jsbsim/fcs/rudder-ap-cmd-norm</property>
			<value>0</value>
		</binding>
	</key>

	<key n="93">
		<name>]</name>
		<desc>Flaps Down</desc>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>mig21.flap_keybind(0)</script>
		</binding>
	</key>

	<key n="91">
		<name>[</name>
		<desc>Flaps Up</desc>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>mig21.flap_keybind(1)</script>
		</binding>
	</key>

	<key n="103">
		<name>g</name>
		<desc>Gear Handle Up</desc>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>mig21.gear_setting(1);</script>
		</binding>
	</key>

	<key n="71">
		<name>G</name>
		<desc>Gear Handle Down</desc>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>mig21.gear_setting(-1);</script>
		</binding>
	</key>

	 <key n="261">
  <name>F5</name>
  <repeatable type="bool">true</repeatable>
  <mod-shift>
   <desc>Reload autopilot configuration</desc>
   <binding>
    <command>nasal</command>
    <script>
     fgcommand('reinit', props.Node.new({ subsystem: "xml-autopilot" }));
     print("xml-autopilot reinitialized!");
    </script>
   </binding>
  </mod-shift>

 </key>

</PropertyList>
