					<tableData>
						0.0	 0.0
						22.0	115.0
					</tableData>
				</table>
			</function>
			<output>electric/inverters/po-750a[1]</output>
		</fcs_function>

		<fcs_function name="trash/electric/inverters/po-1500vt">
			<function>
				<table>
