	<channel name="system_load" execrate="3">
		<!-- the current load on the electrical system, to determine discharge rate -->

		<fcs_function name="trash/electric/discharge/bus_1_load">
			<!-- the sum of the output voltages divided by the input voltage will give us the amount of systems running -->
			<function>
				<quotient>
					<sum>
						<value>1</value>
						<value>1</value>
					</sum>
					<property>electric/buses/bus[0]</property>
				</quotient>
			</function>
			<output>electric/discharge/bus_1_load</output>
		</fcs_function>

