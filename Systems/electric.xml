		</switch>
		
		<switch name="electric/output/blc-syst-shutters-control-2">
			<default value="0"/>
			<test logic="AND" value="electric/buses/bus[2]">
				electric/switches/blc-syst-shutters-control-2 == 1
			</test>
		</switch>
		
		<switch name="electric/output/tac-rls-1-2">
			<default value="0"/>
			<test logic="AND" value="electric/buses/bus[2]">
				electric/switches/tac-rls-1-2 == 1
			</test>
		</switch>
		
		<switch name="electric/output/cabin-air-sup">
			<default value="0"/>
			<test logic="AND" value="electric/buses/bus[2]">
				electric/switches/cabin-air-sup == 1
			</test>
		</switch>
		
		<switch name="electric/output/firing-button">
			<default value="0"/>
			<test logic="AND" value="electric/buses/bus[2]">
				electric/switches/firing-button == 1
