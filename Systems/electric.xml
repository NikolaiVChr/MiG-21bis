		<switch name="electric/output/apy-syst-auto-ctl">
			<default value="0"/>
			<test logic="AND" value="electric/buses/bus[2]">
				electric/switches/apy-syst-auto-ctl == 1
			</test>
		</switch>
		
		<switch name="electric/output/tac-rls-3-4">
			<default value="0"/>
			<test logic="AND" value="electric/buses/bus[2]">
