<?xml version="1.0"?>
<PropertyList>

    <path>arcknob.ac</path>

    <animation>
        <type>knob</type>
        <object-name>arcKnob</object-name>
        <property>/instrumentation/adf/arc-sel</property>
        <factor>-30</factor>
        <offset-deg>105</offset-deg>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>/instrumentation/adf/arc-sel</property>
                <factor>1</factor>
                <min>0</min>
                <max>7</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
	
</PropertyList>