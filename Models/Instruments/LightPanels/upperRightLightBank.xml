<?xml version="1.0"?>
<PropertyList>

    <path>upperRightLightBank.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>dimmerKnob5</object-name>
        <property>instrumentation/warning-lights/dimmer[5]</property>
        <interpolation>
            <entry>
                <ind> 	0.0</ind>
                <dep>   0.0</dep>
            </entry>
            <entry>
                <ind>  1.0</ind>
                <dep> 330.0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m> 0.00813</x-m>
            <y-m> 0.00164</y-m>
            <z-m>-0.04200</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>dimmerKnob5</object-name>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.05</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/warning-lights/dimmer[5]</property>
                <step>0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <action>
            <button>1</button>
            <repeatable>true</repeatable>
            <interval-sec>0.05</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/warning-lights/dimmer[5]</property>
                <step>-0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <action>
            <button>3</button>
            <repeatable>true</repeatable>
            <interval-sec>0.1</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/warning-lights/dimmer[5]</property>
                <step>0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <action>
            <button>4</button>
            <repeatable>true</repeatable>
            <interval-sec>0.1</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/warning-lights/dimmer[5]</property>
                <step>-0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

	<!--lights-->

    <animation>
        <type>material</type>
        <object-name>serviceTankLight</object-name>
        <emission>
            <red>1.0</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>/fdm/jsbsim/warning-lights/service-tank</factor-prop>
        </emission>
    </animation>

	<animation>
        <type>material</type>
        <object-name>firstReheatLight</object-name>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.0</blue>
            <factor-prop>/fdm/jsbsim/warning-lights/first-reheat</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>secondReheatLight</object-name>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.0</blue>
            <factor-prop>/fdm/jsbsim/warning-lights/second-reheat</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>dcGenOffLight</object-name>
        <emission>
            <red>1.0</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>/fdm/jsbsim/warning-lights/dc-gen-off</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>acGenOffLight</object-name>
        <emission>
            <red>1.0</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>/fdm/jsbsim/warning-lights/ac-gen-off</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>fireLight</object-name>
        <emission>
            <red>1.0</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>/fdm/jsbsim/warning-lights/fire</factor-prop>
        </emission>
    </animation>

      <animation>
          <type>material</type>
          <object-name>jetNozzleOpenLight</object-name>
          <emission>
              <red>0.0</red>
              <green>1.0</green>
              <blue>0.0</blue>
              <factor-prop>/fdm/jsbsim/warning-lights/jet-nozzle-open</factor-prop>
          </emission>
      </animation>


        <animation>
            <type>material</type>
            <object-name>switchOffIgnitionLight</object-name>
            <emission>
                <red>1.0</red>
                <green>0.0</green>
                <blue>0.0</blue>
                <factor-prop>/fdm/jsbsim/warning-lights/switch-off-ignition</factor-prop>
            </emission>
        </animation>

    <animation>
        <type>material</type>
        <object-name>watchBkupSystPresLight</object-name>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.0</blue>
            <factor-prop>/fdm/jsbsim/warning-lights/watch-bkup-syst-pres</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>watchMainSystPresLight</object-name>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.0</blue>
            <factor-prop>/fdm/jsbsim/warning-lights/watch-main-syst-pres</factor-prop>
        </emission>
    </animation>

</PropertyList>
