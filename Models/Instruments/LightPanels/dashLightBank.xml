<?xml version="1.0"?>
<PropertyList>

   <path>dashLightBank.ac</path>


   <animation>
      <type>rotate</type>
      <object-name>dimmerKnob1</object-name>
      <property>instrumentation/warning-lights/dimmer[6]</property>
      <interpolation>
         <entry><ind>0.0</ind><dep>0.0</dep></entry>
         <entry><ind>1.0</ind><dep>330.0</dep></entry>
      </interpolation>
      <center>
         <x-m> 0.00813</x-m>
         <y-m> 0.00164</y-m>
         <z-m>-0.04200</z-m>
      </center>
      <axis>
         <x>-1</x>
         <y>0</y>
         <z>0</z>
      </axis>
   </animation>

   <animation>
      <type>pick</type>
      <object-name>dimmerKnob1</object-name>
      <action>
         <button>0</button>
         <repeatable>true</repeatable>
         <interval-sec>0.05</interval-sec>
         <binding>
            <command>property-adjust</command>
            <property>instrumentation/warning-lights/dimmer[6]</property>
            <step>0.1</step>
            <min>0</min>
            <max>1</max>
            <wrap>false</wrap>
         </binding>
      </action>
      <action>
         <button>1</button>
         <repeatable>true</repeatable>
         <interval-sec>0.05</interval-sec>
         <binding>
            <command>property-adjust</command>
            <property>instrumentation/warning-lights/dimmer[6]</property>
            <step>-0.1</step>
            <min>0</min>
            <max>1</max>
            <wrap>false</wrap>
         </binding>
      </action>
      <action>
         <button>3</button>
         <repeatable>true</repeatable>
         <interval-sec>0.1</interval-sec>
         <binding>
            <command>property-adjust</command>
            <property>instrumentation/warning-lights/dimmer[6]</property>
            <step>0.1</step>
            <min>0</min>
            <max>1</max>
            <wrap>false</wrap>
         </binding>
      </action>
      <action>
         <button>4</button>
         <repeatable>true</repeatable>
         <interval-sec>0.1</interval-sec>
         <binding>
            <command>property-adjust</command>
            <property>instrumentation/warning-lights/dimmer[6]</property>
            <step>-0.1</step>
            <min>0</min>
            <max>1</max>
            <wrap>false</wrap>
         </binding>
      </action>
   </animation>

   <animation>
      <type>material</type>
      <object-name>markerBeaconLight</object-name>
      <emission>
         <red>0.0</red>
         <green>1.0</green>
         <blue>0.0</blue>
         <factor-prop>/fdm/jsbsim/warning-lights/marker</factor-prop>
      </emission>
   </animation>

   <animation>
      <type>material</type>
      <object-name>noseconeExtendedLight</object-name>
      <emission>
         <red>0.0</red>
         <green>1.0</green>
         <blue>0.0</blue>
         <factor-prop>/fdm/jsbsim/warning-lights/cone-extended</factor-prop>
      </emission>
   </animation>

   <animation>
      <type>material</type>
      <object-name>blcLandingModeLight</object-name>
      <emission>
         <red>0.0</red>
         <green>1.0</green>
         <blue>0.0</blue>
         <factor-prop>/fdm/jsbsim/warning-lights/stab-for-landing</factor-prop>
      </emission>
   </animation>

   <animation>
      <type>material</type>
      <object-name>trimNeutralLight</object-name>
      <emission>
         <red>0.0</red>
         <green>1.0</green>
         <blue>0.0</blue>
         <factor-prop>/fdm/jsbsim/warning-lights/trim-neutral</factor-prop>
      </emission>
   </animation>

</PropertyList>