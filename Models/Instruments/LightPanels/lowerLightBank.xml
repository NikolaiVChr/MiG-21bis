<?xml version="1.0"?>
<PropertyList>

    <path>lowerLightBank.ac</path>

	<!--dimmer knob-->
	<animation>
        <type>rotate</type>
        <object-name>dimmerKnob2</object-name>
        <property>instrumentation/warning-lights/dimmer[2]</property>
        <interpolation>
            <entry>
                <ind> 	0.0</ind>
                <dep>   0.0</dep>
            </entry>
            <entry>
                <ind>  1.0</ind>
                <dep> 330.0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m> 0.00571</x-m>
            <y-m> 0.00010</y-m>
            <z-m>-0.03048</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>dimmerKnob2</object-name>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.05</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/warning-lights/dimmer[2]</property>
                <step>0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <action>
            <button>1</button>
            <repeatable>true</repeatable>
            <interval-sec>0.05</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/warning-lights/dimmer[2]</property>
                <step>-0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <action>
            <button>3</button>
            <repeatable>true</repeatable>
            <interval-sec>0.1</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/warning-lights/dimmer[2]</property>
                <step>0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <action>
            <button>4</button>
            <repeatable>true</repeatable>
            <interval-sec>0.1</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/warning-lights/dimmer[2]</property>
                <step>-0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <!-- lights -->

	<animation>
		<type>material</type>
		<object-name>no1rocket0light</object-name>
		<emission>
			<red>0.0</red>
			<green>1.0</green>
			<blue>0.0</blue>
			<factor-prop>/fdm/jsbsim/warning-lights/no-1-rkt-zero-psn</factor-prop>
		</emission> 
	</animation>

	<animation>
		<type>material</type>
		<object-name>no2rocket0light</object-name>
		<emission>
			<red>0.0</red>
			<green>1.0</green>
			<blue>0.0</blue>
			<factor-prop>/fdm/jsbsim/warning-lights/no-2-rkt-zero-psn</factor-prop>
		</emission> 
	</animation>

	<animation>
		<type>material</type>
		<object-name>no3rocket0light</object-name>
		<emission>
			<red>0.0</red>
			<green>1.0</green>
			<blue>0.0</blue>
			<factor-prop>/fdm/jsbsim/warning-lights/no-3-rkt-zero-psn</factor-prop>
		</emission> 
	</animation>

	<animation>
		<type>material</type>
		<object-name>no4rocket0light</object-name>
		<emission>
			<red>0.0</red>
			<green>1.0</green>
			<blue>0.0</blue>
			<factor-prop>/fdm/jsbsim/warning-lights/no-4-rkt-zero-psn</factor-prop>
		</emission> 
	</animation>
	
</PropertyList>