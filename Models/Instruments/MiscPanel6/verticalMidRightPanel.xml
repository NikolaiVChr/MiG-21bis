    <offset-deg>25</offset-deg>
    <center>
      <x-m>-0.007692</x-m>
      <y-m>-0.064599</y-m>
      <z-m> 0.089658</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>

    <action>
      <binding>
        <command>property-toggle</command>
        <property>/fdm/jsbsim/electric/switches/rvmsp/cone</property>
      </binding>
    </action>
  </animation>

